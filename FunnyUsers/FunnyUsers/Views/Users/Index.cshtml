@using FunnyUsers.Models;
@model IEnumerable<ApplicationUser>


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>UserName</td>
            <td>Email</td>
            <td>Department</td>
            <td>Role</td>
            <td>SOME ROLE</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
            <td>@user.UserName
            <td>@user.Email</td>
            <td>@user.Department</td>
            <td>@user.Role</td>
            <td>s</td>
            <td>
                <partial name="_PartialView" model="user.Id"/>
            </td>
        </tr>

        }


    </tbody>
</table>  